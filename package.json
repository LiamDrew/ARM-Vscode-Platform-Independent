{
	"name": "arm-assembly",
	"displayName": "Liam's Arm Assembly Preferences",
	"description": "Arm assembly syntax with platform independent comments, forked from dan-c-underwood/vscode-arm",
	"version": "1.0.0",
	"publisher": "LiamDrew",
	"icon": "images/icon.png",
	"author": {
		"name": "Liam Drew"
	},
	"forkedFrom": {
		"name": "Dan Underwood",
		"url": "https://github.com/dan-c-underwood/vscode-arm"
	},
	"categories": [
		"Programming Languages"
	],
	"engines": {
		"vscode": "^0.10.1"
	},
	"galleryBanner": {
		"color": "#0084A9",
		"theme": "dark"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/LiamDrew/ARM-Vscode-Platform-Independent"
	},
	"contributes": {
		"languages": [
			{
				"id": "arm",
				"aliases": [
					"ARM",
					"arm",
					"Arm"
				],
				"extensions": [
					".s",
					".S",
					".asm",
					".sx"
				],
				"configuration": "./language-configuration.json"
			}
		],
		"grammars": [
			{
				"language": "arm",
				"scopeName": "source.arm",
				"path": "./syntaxes/arm.tmlanguage.json"
			}
		]
	}
}
